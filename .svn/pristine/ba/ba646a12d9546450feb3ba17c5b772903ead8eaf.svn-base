ul,li{
    list-style:none;
}
a,a:hover,a:visited,a:link,a:active{
    text-decoration: none;
    color: #000;
    border:0;
    outline:none;
    text-decoration: none !important;
}
a:focus{outline-offset:0px!important;}
input,button{
    padding:5px;
    outline:none!important;
    text-decoration:none;
}
*,body, input, button {
    font-family: '微软雅黑', 'yahei', ubuntu, helvetica, arial;
    font-size: 16px;
    font-style: normal;
}
body{
    background-color:#fff!important;
    /*overflow-x:hidden;*/
}
*{
    margin:0;
    padding:0;
    box-sizing:border-box;
    -moz-box-sizing:border-box;
    -webkit-box-sizing:border-box;
    -ms-box-sizing:border-box;
}
.fl{float: left;}
.fr{float: right;}
.clearfix:after { content: ""; display: block; height: 0; clear: both; visibility: hidden; }
.clearfix { display: inline-table; }
*html .clearfix { height: 1%; }			/*兼容ie6*/
.clearfix { display: block; }
*+html .clearfix { min-height: 1%; }	/*兼容ie7*/



/*nav导航*/
.z-nav.nav{
    width: 100%;
    min-width: 1250px;
    background:#fff;
    position: inherit;
    border-bottom: 1px solid #cccccc;
}
.z-nav.nav .z-index-nav-ul li a{
    color: #1a1a1a;
}
.z-nav.nav .z-index-logo{
    position: inherit;
    float: left;
    margin-top: 23px;
}
.z-nav .z-index-logo a{
    display: block;
}
.z-nav .z-index-nav{
    width: 100%;
    margin: 0 auto;
    text-align: center;
}
.z-nav.nav .z-index-nav{
    width: 1200px;
    margin: 0 auto;
    text-align: center;
}
.z-nav .z-index-nav-ul{
    overflow: hidden;
    display: inline-block;
    margin-bottom: 0;
}

.z-nav .z-index-nav-ul li{
    width: 120px;
    height: 72px;
    line-height: 72px;
    text-align: center;
    float: left;

}
.z-nav .z-index-nav-ul li:last-child{
    width: 0px;  /*最后一个是空的li，如果去掉就注释这一行*/
}
.z-nav .z-index-nav-ul li a{
    margin: 0 auto;
    width: 70px;
    height: 57px;
    display: block;
    font-size: 16px;
    color: #fff;
}
.z-nav .z-index-nav-ul li.currents a,.z-nav .z-index-nav-ul li:hover a{
    border-bottom: 2px solid #df222a;
    color: #df222a;
}
/*nav导航 结束*/

/*footer 与首页的footer样式有区别 这里写footer以下的样式必须加.footer*/
.footer{
    width:100%;
    min-width: 1250px;
    background: #e6e6e6;
    line-height: 24px;
    color: #999;
}
.footer .z-footer{
    width:1250px;
    margin:0 auto;
    padding: 65px 0 75px 95px;
    color: #999;
}
.footer .z-footer-left{
    float: left;
    width: 530px;
}
.footer .z-footer-center{
    float: left;
    width: 55px;
    height: 300px;
    border-right: 1px solid #999;
    margin-right: 70px;
    margin-top: 35px;
}
.footer .z-footer-c-t{
    margin-bottom: 0;
}
.footer .z-footer-c-c{
    margin: 21px 0;
}
.footer .z-footer-right{
    float: left;
}
.footer .z-footer-right p{
    margin-bottom: 0;
}
.footer .z-footer-right .z-footer-erweima{
    margin: 18px 0 15px 0;
}
.footer .z-footer-right .z-footer-erweima img{
	width: 124px;
	height: 124px;
}
.footer .z-footer-right .z-footer-teshu{
    text-indent: 0.4em;
    margin-bottom: 20px;
}
/*footer end*/


/*联系*/
.consult-content {
    width: 100%;
    background: #f5f5f5;
}
.contact-content-in{
	width: 1500px;
    margin: 0 auto;
    padding: 0;
}
.consult-list {
    width: 100%;
    border: 1px solid #e3e3e3;
    background: #fff;
    padding: 20px 0 20px 20px;
    margin-bottom: 25px;
}
.clearfix {
    display: block;
}
.consult-list img {
    float: left;
}
.list-right {
    float: left;
    width: 860px;
    margin-left: 30px;
}
.contactdiv h3{
	font-size: 24.5px;
	font-weight: 400;
	margin: 15px 0px;
}
.contactdiv p{
	font-size: 18px;
	margin: 0px 0px 10px 0px;
}

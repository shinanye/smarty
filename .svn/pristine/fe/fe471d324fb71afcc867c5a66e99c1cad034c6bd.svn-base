/*-----------------------------consult 咨询 start--------------------------------------*/
.consult-content{
    width: 100%;
    background: #f5f5f5;
}
.consult-content-in{
    width: 1200px;
    margin: 0 auto;
    padding: 25px 0;
}
.consult-list{
    width: 100%;
    border: 1px solid #e3e3e3;
    background: #fff;
    padding: 20px 0 20px 20px;
    margin-bottom: 25px;
}
.consult-list img{
    float: left;
}
.list-right{
    float: left;
    width: 860px;
    margin-left: 25px;
}
.list-right h1{
    font-weight: 500;
    color: #333;
    font-size: 24px;
}
.list-right p{
    margin-top: 32px;
    line-height: 30px;
    color: #808080;
    font-size: 16px;
}
.list-right ul li{
    color: #808080;
    font-size: 16px;
}
.list-right ul li:nth-child(1){
    float: left;
}
.list-right ul li:nth-child(2){
    float: right;
}
.list-right ul li:nth-child(2) a{
    color: #be182c;
}

.consult-details{
    border: 1px solid #e3e3e3;
    background: #fff;
    padding: 30px 20px;
}
.consult-details h1{
    font-weight: 500;
    color: #333;
    font-size: 24px;
}
.consult-details .details-time{
    display: block;
    color: #999999;
    margin: 20px 0;
}
.consult-details p{
    color: #808080;
    line-height: 25px;
}

/*-----------------------------consult 咨询 end--------------------------------------*/






















h1,h2,h3,h4,h5,h6{
    font-weight: normal;
}
*{
    margin:0;
    padding:0;
}

/*-----------------------------service页面--------------------------------------*/
.service-con  .h3-con{
    width: 100%;
    min-width: 1200px;
    background: #fff;
}
.h3-con h3{
    width: 1200px;
    margin: 0 auto;
    font-size: 18px;
    line-height: 53px;
    height: 53px;
    padding: 0 10px;
}
.service-con .yingyong-con{
    width: 100%;
    min-width: 1200px;
    background: #f2f2f2;
}
.yingyong-con .yingyong{
    width: 1200px;
    margin: 0 auto;
    padding: 0 10px;
}
.yingyong-con .yingyong p{
    height: 47px;
    line-height: 47px;
    font-size: 16px;
}
.yingyong-con .yingyong ul{
    padding: 5px 0 10px;
}
.yingyong-con .yingyong ul li{
    line-height: 24px;
}
.yingyong-con .yingyong ul li a{
    color: #be182c;
    font-size: 14px;
}
.service-con .main{
    width: 1200px;
    margin: 0 auto;
    padding: 25px 10px 0;
}
.service-con .main p,.service-con .main h4{
    line-height: 24px;
    font-size: 14px;
    color: #4c4c4c;
    margin-bottom: 22px;
}
.service-con .main p.m-bott0{
    margin-bottom: 0px;
}
.service-con .main p.p-zs{
    font-size: 16px;
    color: #1a1a1a;
    margin:55px 0 25px 0;
}
/*-----------------------------service页面 end--------------------------------------*/

/*-----------------------------joinUs页面--------------------------------------*/
.joinUs-con{

}
.joinUs-banner{
    width: 100%;
}
.joinUs-banner img{
    display: block;
    margin: 0 auto;
}
.joinUs-con .joinUs-text{
    width: 1200px;
    margin: 0 auto;
    padding-left: 10px;
}
.joinUs-con .joinUs-text h4{
    font-size: 18px;
    color: #333;
    line-height: 80px;
    height: 80px;
    text-align: center;
}
.joinUs-con .joinUs-text p{
    font-size: 14px;
    color: #333;
    line-height: 24px;
    text-align: center;
}
.joinUs-con .joinUs-text p.join-p-ts{
    padding-bottom: 30px;
}
.joinUs-con .joinUs-recruit{
    width: 100%;
    min-width: 1200px;
    background:#1d1d1d url(../images/joinUs/zhaopin_zhiwei_bg.png) no-repeat center top;
    color: #fff;
}
.joinUs-con ol li{
    line-height: 45px;
    cursor: pointer;
}
.joinUs-con ol li a{
    color: #fff;
    display: inline-block;
    padding-right: 20px;
    background: #272727 url(../images/joinUs/zhaopin_quan.png) no-repeat left center;
    text-indent: 2em;
}
.joinUs-con ol li a:hover{
    background: #272727 url(../images/joinUs/zhaopin_quan_hover.png) no-repeat left center;
}
.joinUs-recruit .recruit-tit{
    width: 1200px;
    margin: 0 auto;
    padding:25px 0 30px 10px;
}
.joinUs-recruit .recruit-tit p{
    font-size: 14px;
    line-height: 30px;
}
.joinUs-recruit .recruit-con{
    width: 1200px;
    margin: 0 auto;
    padding: 0 10px 60px;
}
.recruit-con ol{
    width: 470px;
}
.recruit-con ol li.current a{
    color: #de2128;
    background: #272727 url(../images/joinUs/zhaopin_quan_hover.png) no-repeat left center;
}
.recruit-con ol li.current{
    background:url(../images/joinUs/zhaopin_line.png) no-repeat left center;
}
.recruit-con ul{
	width: 528px;
}
.recruit-con ul li{
    display: none;
    line-height: 24px;
}
.recruit-con ul li.currents{
    display: block;
}
.recruit-con ul li p{
    font-size: 18px;
    margin-bottom: 26px;
}
.recruit-con ol li,.recruit-con ul li{
    font-size: 14px;
}
/*-----------------------------joinUs页面 end--------------------------------------*/

/*-----------------------------casus 案例页面--------------------------------------*/
.casus-con{

}
.casus{
    width: 1200px;
    margin: 0 auto;
}
.casus ol{
    height: 60px;
    line-height: 60px;
    border-bottom: 1px solid #ccc;
}
.casus ol li{
    float: left;
    margin-right: 40px;
}
.casus ol li:last-child{
    margin-right: 0;
}
.casus ol li a{
    display: block;
    font-size: 14px;
    color: #333333;
    height: 60px;
}
.casus ol li.current a{
    border-bottom: 4px solid #be182c;
    color: #be182c;
}
.casus .casus-ul-con{
    padding-bottom: 108px;
}
.casus .casus-ul-con > li{
    display: none;
    line-height: 24px;
}
.casus .casus-ul-con > li.currents{
    display: block;
}
.casus .casus-ul-con > li > .casus-ul-li-tit{
    font-size: 14px;
    color: #666;
    padding: 10px 0 20px;
    width: 100%;
}
.casus-ul-con-ul{
    width: 100%;
}
.casus-ul-con-ul li{
    float: left;
    width: 33.333%;
    overflow: hidden;
}
.casus-ul-con-ul li a{
    display: block;
    position: relative;
}
.casus-ul-con-ul li a .casus-li-posi{
    display: block;
    width: 100%;
    height: 100%;
    padding: 25px 28px 0 30px;
    font-size: 14px;
    color: #fff;
    background: rgba(0,0,0,0.5);
    position: absolute;
    left: 0;
    bottom: -100%;
}
.casus-ul-con-ul li a img{
    display: block;
}

/*-----------------------------casus 案例页面 end--------------------------------------*/
/*-----------------------------product 服务页面 start--------------------------------------*/

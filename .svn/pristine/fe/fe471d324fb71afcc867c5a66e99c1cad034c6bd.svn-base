    bottom: 100px;
    line-height: 25px;
    width: 1000px;
    text-align: center;
    left: 50%;
    margin-left: -500px;
    color: #fff;
}
.u-product-content{
    width: 1200px;
    margin:0 auto;
    /*padding: 20px 0;*/
   margin-top: 25px;
}
.product-content1{
	margin-bottom: 30px !important;
}
.u-product-content div{
    float: left;
    width: 708px;
    height: 250px;
    margin-left: 70px;
    overflow: hidden;
}
.u-product-content img{
    float: left;
    /*margin-left: 140px;*/
   	width: 420px;
    height: 250px;
}
.u-product-content div h1{
    font-weight: 500;
    color: #333;
